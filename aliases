[alias]
	a = add
	ac = commit --all --verbose
	acm = commit --all --message
	ap = add --patch
	b = branch
	bm = branch --merged
	bu = branch --unmerged
	bv = branch -v
	bvv = branch -vv
	ci = commit -v
	cia = commit --amend -v
	ciao = commit --amend --no-edit
	cim = commit --message
	cif = !sh -c 'git commit --fixup=$1' -
	cir = !sh -c 'git commit --reuse-message=${1:-HEAD}' -
	co = checkout
	cob = checkout -b
	cop = checkout --patch
	d = diff
	dc = diff --cached
	dm = !sh -c 'diffuse -m "$@" &' -
	f = fetch --prune
	fa = fetch --all --prune
	ffs = flow feature start
	ffp = flow feature publish
	fff = flow feature finish
	frs = flow release start
	frp = flow release publish
	frf = flow release finish
	l = log --numstat
	lp = log --patch
	m = merge
	ma = merge --abort
	mf = merge --ff
	ph = push
	phf = push --force
	phu = push --set-upstream
	pl = pull
	plr = pull --rebase
	q = reset
	qh = reset HEAD
	qhp = reset HEAD^
	r = rebase
	ri = rebase --interactive --autosquash
	ra = rebase --abort
	rc = rebase --continue
	rs = rebase --skip
	rma = rebase master
	rmi = rebase master --interactive --autosquash
	s = status
